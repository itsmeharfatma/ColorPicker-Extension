{
  "manifest_version": 3,
  "name": "ColorPicker",
  "version": "0.1.0",
  "description": "Easily pick colors from your browser screen.",
  "action": {
    "default_icon": {              
      "16": "icon16.png",   
      "32": "icon32.png",
      "48": "icon48.png",
      "66": "icon66.png",
      "96": "icon96.png"
    },
    "default_title": "ColorPicker",
    "default_popup": "index.html"
  }
}